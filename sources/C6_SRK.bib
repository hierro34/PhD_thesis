	author = {Sanderse, B. and Koren, B.},
	year = {2012},
	keywords = {Differential-algebraic equations, incompressible Navier-Stokes equations, Moving meshes, Runge-Kutta method, Temporal accuracy, time integration},
	pages = {3041--3063},
	file = {ScienceDirect Full Text PDF:/home/ocolomes/.mozilla/firefox/x6wyo4f0.default/zotero/storage/PNZ7MU7T/Sanderse y Koren - 2012 - Accuracy analysis of explicit Runge-Kutta methods .pdf:application/pdf;ScienceDirect Snapshot:/home/ocolomes/.mozilla/firefox/x6wyo4f0.default/zotero/storage/9JXDMKWF/S0021999111006838.html:text/html}
}

