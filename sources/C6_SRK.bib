	journal = {Journal of Computational Physics},
	author = {Veneziani, Alessandro and Villa, Umberto},
	year = {2013},
	keywords = {incompressible Navier-Stokes, Inexact algebraic factorizations, Time adaptivity},
	pages = {359--375},
	file = {ScienceDirect Full Text PDF:/home/ocolomes/.mozilla/firefox/x6wyo4f0.default/zotero/storage/CS68Q3GE/Veneziani y Villa - 2013 - ALADINS An ALgebraic splitting time ADaptive solv.pdf:application/pdf;ScienceDirect Snapshot:/home/ocolomes/.mozilla/firefox/x6wyo4f0.default/zotero/storage/AFC94RS2/S0021999112007310.html:text/html}
}

@article{sanderse_accuracy_2012,
