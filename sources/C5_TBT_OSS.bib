	month = jun,
	year = {2013},
	keywords = {Classical Continuum Physics, Coarse resolution, Compressible turbulence, Computational Science and Engineering, De-aliasing, Engineering Fluid Dynamics, High-order discontinuous Galerkin, stabilization, Taylor-Green vortex},
	pages = {221--237},
	file = {Full Text PDF:/home/ocolomes/.mozilla/firefox/x6wyo4f0.default/zotero/storage/WGARWZ8X/Gassner y Beck - 2013 - On the accuracy of high-order discretizations for .pdf:application/pdf;Snapshot:/home/ocolomes/.mozilla/firefox/x6wyo4f0.default/zotero/storage/RV7UQNFX/10.html:text/html}
}

@article{codina_analysis_2008,
	title = {Analysis of a stabilized finite element approximation of the {Oseen} equations using orthogonal subscales},
	volume = {58},
	issn = {0168-9274},
	url = {http://www.sciencedirect.com/science/article/pii/S0168927406002194},
	doi = {10.1016/j.apnum.2006.11.011},
	abstract = {In this paper we present a stabilized finite element formulation to solve the Oseen equations as a model problem involving both convection effects and the incompressibility restriction. The need for stabilization techniques to solve this problem arises because of the restriction in the possible choices for the velocity and pressure spaces dictated by the inf-sup condition, as well as the instabilities encountered when convection is dominant. Both can be overcome by resorting from the standard Galerkin method to a stabilized formulation. The one presented here is based on the subgrid scale concept, in which unresolvable scales of the continuous solution are approximately accounted for. In particular, the approach developed herein is based on the assumption that unresolved subscales are orthogonal to the finite element space. It is shown that this formulation is stable and optimally convergent for an adequate choice of the algorithmic parameters on which the method depends.},
	number = {3},
	urldate = {2014-10-29},
	journal = {Applied Numerical Mathematics},
	author = {Codina, Ramon},
	month = mar,
	year = {2008},
	keywords = {Convection-dominated flows, Inf-sup condition, Orthogonal subscales, Stabilized finite element methods},
	pages = {264--283},
	file = {ScienceDirect Full Text PDF:/home/ocolomes/.mozilla/firefox/x6wyo4f0.default/zotero/storage/ZKJGD26F/Codina - 2008 - Analysis of a stabilized finite element approximat.pdf:application/pdf;ScienceDirect Snapshot:/home/ocolomes/.mozilla/firefox/x6wyo4f0.default/zotero/storage/EWSRZN99/S0168927406002194.html:text/html}
}

