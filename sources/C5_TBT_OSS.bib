	author = {Yano, Masayuki and Darmofal, David L.},
	month = nov,
	year = {2010},
	keywords = {BDDC, Domain decomposition methods, Galerkin Least-Squares, High-order methods, preconditioners},
	pages = {2958--2969},
	file = {ScienceDirect Full Text PDF:/home/sbadia/.mozilla/firefox/38g6z3d4.default/zotero/storage/863X62BJ/Yano and Darmofal - 2010 - BDDC preconditioning for high-order Galerkin Least.pdf:application/pdf;ScienceDirect Snapshot:/home/sbadia/.mozilla/firefox/38g6z3d4.default/zotero/storage/S5SZ9SWW/Yano and Darmofal - 2010 - BDDC preconditioning for high-order Galerkin Least.html:text/html}
}

